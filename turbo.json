{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev:client": {
      "cache": false
    },
    "dev:client:mf": {
      "cache": false
    },
    "dev:backend": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["dev:backend", "dev:client"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "build": {
      "outputMode": "new-only"
    },
    "lint": {}
  },
  "globalDependencies": [".eslintrc.js", ".env"]
}
